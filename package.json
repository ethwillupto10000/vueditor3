  },
  "homepage": "https://github.com/ethwillupto10000/vueditor3#readme",
  "keywords": [
    "editor",
    "vue3 editor",
    "富文本编辑器",
    "vue3.x 编辑器",
    "vue3富文本编辑器"
  ],
  "dependencies": {
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-router": "^4.1.1"
